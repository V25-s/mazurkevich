    "years = ['2017 г.', '2018 г.', '2019 г.', '2020 г.', '2021 г.', '2022 г.', '2023 г.']\n",
    "\n",
    "in_total = df_in[years].sum()\n",
    "out_total = df_out[years].sum()\n",
    "\n",
    "max_in_year = in_total.idxmax()\n",
    "max_in_value = in_total.max()\n",
    "\n",
    "max_out_year = out_total.idxmax()\n",
    "max_out_value = out_total.max()\n",
    "\n",
    "print(\"Год с максимальным суммарным прибытием:\", max_in_year, max_in_value)\n",
