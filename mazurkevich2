    "df_in_2023 = df[['регион', 'округ', '2023 г.']].rename(\n",
    "    columns={'регион': 'Region', 'округ': 'District', '2023 г.': 'Arrivals'}\n",
    ")\n",
    "df_out_2023 = df2[['регион', 'округ', '2023 г.']].rename(\n",
    "    columns={'регион': 'Region', 'округ': 'District', '2023 г.': 'Departures'}\n",
    ")\n",
    "\n",
    "merged_2023 = pd.merge(df_in_2023, df_out_2023, on=['Region', 'District'])\n",
    "merged_2023['Balance'] = merged_2023['Arrivals'] - merged_2023['Departures']\n",
    "merged_2023['Region'] = merged_2023['Region'].replace(region_map)\n",
    "\n",
    "merged_2023_sorted = merged_2023.sort_values('Balance')\n",
    "plt.figure(figsize=(12, 6))\n",
    "sns.barplot(data=merged_2023_sorted, x='Region', y='Balance')\n",
    "\n",
    "plt.xticks(rotation=90)\n",
    "for label in plt.gca().get_xticklabels():\n",
    "    label.set_fontname('Times New Roman')\n",
    "for label in plt.gca().get_yticklabels():\n",
    "    label.set_fontname('Times New Roman')\n",
    "\n",
    "plt.title('Regional Migration Balance (2023)', fontname='Times New Roman')\n",
    "plt.ylabel('Migration Balance', fontname='Times New Roman')\n",
    "plt.xlabel('Region', fontname='Times New Roman')\n",
    "plt.grid(True)\n",
