   "source": [
    "\n",
    "def describe_by_year(df, value_label):\n",
    "    stats = df.loc[:, ['2017 г.', '2018 г.', '2019 г.', '2020 г.', '2021 г.', '2022 г.', '2023 г.']].describe(percentiles=[.25, .5, .75]).T\n",
    "    stats = stats.rename_axis('Год').reset_index()\n",
    "    stats['Показатель'] = value_label\n",
    "    return stats\n",
    "\n",
    "stats_in = describe_by_year(df_in, 'Прибыло')\n",
    "stats_out = describe_by_year(df_out, 'Выбыло')\n",
    "stats_vrp = describe_by_year(df_vrp, 'ВРП')\n",
    "\n",
    "all_stats = pd.concat([stats_in, stats_out, stats_vrp])\n",
    "\n",
    "print(all_stats[['Показатель', 'Год', 'count', 'mean', 'std', 'min', '25%', '50%', '75%', 'max']])"
   ]
  },
