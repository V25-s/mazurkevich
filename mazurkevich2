   ],
   "source": [
    "df_in = pd.read_excel('прибыло.xlsx')   \n",
    "df_grp = pd.read_excel('врп.xlsx')     \n",
    "\n",
    "df_in_2023 = df_in[['регион', 'округ', '2023 г.']].rename(\n",
    "    columns={'регион': 'Region', 'округ': 'District', '2023 г.': 'Arrivals'}\n",
    ")\n",
    "df_grp_2023 = df_grp[['регион', 'округ', '2023 г.']].rename(\n",
    "    columns={'регион': 'Region', 'округ': 'District', '2023 г.': 'GRP'}\n",
    ")\n",
    "\n",
    "# Merge data on Region and District\n",
    "merged = pd.merge(df_in_2023, df_grp_2023, on=['Region', 'District'])\n",
    "\n",
    "# (Optional) Replace Russian region names with English\n",
    "merged['Region'] = merged['Region'].replace(region_map)\n",
    "\n",
    "# Pearson correlation\n",
    "corr_coeff, p_value = pearsonr(merged['Arrivals'], merged['GRP'])\n",
    "\n",
    "# Plot\n",
    "plt.figure(figsize=(8, 6))\n",
    "plt.scatter(merged['GRP'], merged['Arrivals'])\n",
    "plt.xlabel('GRP, million rubles', fontname='Times New Roman')\n",
    "plt.ylabel('Population arrivals', fontname='Times New Roman')\n",
    "plt.title(f'Correlation between GRP and Migration Arrivals (2023)\\n r = {corr_coeff:.2f}, p = {p_value:.2g}', fontname='Times New Roman')\n",
    "plt.grid(True)\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Print output\n",
    "print(merged[['Region', 'GRP', 'Arrivals']])\n",
    "print(f\"Pearson correlation coefficient: {corr_coeff:.2f}, p-value: {p_value:.2g}\")\n",
    "\n",
    "if corr_coeff > 0 and p_value < 0.05:\n",
    "    print(\"There is a statistically significant positive association between GRP and migration arrivals.\")\n",
    "else:\n",
    "    print(\"There is no statistically significant positive association.\")"
   ]
